{
  "additional-repositories": [],
  "archinstall-language": "English",
  "audio": "pipewire",
  "bootloader": "systemd-bootctl",
  "custom_commands": [
    "mkdir /home/sioel0/{Documents, Downloads, Pictures, Projects}",
    "cd /home/sioel0/Projects; git clone https://github.com/sioel0/arch-installer"
  ],
  "debug": false,
  "harddrives": [
    "HDD"
  ],
  "hostname": "",
  "kernels": [
    "linux"
  ],
  "keyboard-layout": "us",
  "nic": {
    "dhcp": true,
    "dns": null,
    "gateway": null,
    "iface": null,
    "ip": null,
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "packages":  [
    "ansible",
    "git"
  ],
  "profile": "minimal",
  "silent": false,
  "swap": true,
  "sys-encoding": "UTF-8",
  "sys-language": "en_US",
  "timezone": "Europe/Rome",
  "version": "2.6.0"
