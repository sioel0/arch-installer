{
  "additional-repositories": [],
  "archinstall-language": "English",
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader": "systemd-boot",
  "config_version": "2.6.0",
  "custom_commands": [
    "mkdir /home/sioel0/{Documents, Downloads, Pictures, Projects}",
    "cd /home/sioel0/Projects; git clone https://github.com/sioel0/arch-installer"
  ],
  "debug": false,
  "hostname": "HOSTNAME",
  "kernels": [
    "linux"
  ],
  "locale_config": {
    "kb_layout": "us",
    "sys_enc": "UTF-8",
    "sys_lang": "en_US"
  },
  "network_config": {
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "packages":  [
    "ansible",
    "git"
  ],
  "parallel downloads": 0,
  "profile_config": {
    "gfx_driver": null,
    "greeter": null,
    "profile": {
      "custom_settings": {},
      "details": [],
      "main": "Minimal"
    }
  },
  "script": "guided",
  "silent": false,
  "swap": true,
  "timezone": "Europe/Rome",
  "version": "2.6.0"
}